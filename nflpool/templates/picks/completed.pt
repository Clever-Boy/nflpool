<div metal:use-macro="view.layout">
    <div metal:fill-slot="main_content">


        <div class="container">
            <div class="row upcoming">
                <div class="col-md-3"></div>
                <div class="col-md-6">


                    <h1>Your picks are complete</h1>

                    <p>Your picks are complete for the ${season} season.</p>

                    <p>Your picks:


                    <div class="player-picks" tal:repeat="a user_picks">
                        <p>Season: ${a.season} </p>
                        <p>Date submitted: ${a.date_submitted} </p>
                        <p>AFC Winner: ${a.afc_east_first} </p>

                    </div>
                    <p>Good luck!</p>

                </div>
            </div>

        </div>

    </div>

</div>
